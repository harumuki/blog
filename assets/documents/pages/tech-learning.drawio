        <mxCell id="T3fCoR8DOI1882rIEYq2-10" value="A Foundation" style="rounded=1;whiteSpace=wrap;html=1;strokeColor=#432D57;strokeWidth=2;fillColor=#76608a;fontColor=#ffffff;fontStyle=1" vertex="1" parent="1">
